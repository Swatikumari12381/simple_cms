
<h1>All sections  <%= link_to('pages', {controller:'pages',action: 'index'}) %></h1> 
<h1><%= link_to('subject', {controller:'subjects',action: 'index'}) %></h1>




<%= link_to ' Add new section', new_section_path %>
<table style="width:50%">
  <tr>
    <th>id</th>
    <th>page_id</th>
    <th>name</th>

    <th>position</th>
    <th>visible</th>
    <th>contentt ype</th>
    <th>content</th>
    <th>Action</th>

  </tr>
<% @section.each do |o| %>
<tr>
  <td><%= o.id  %></td>
  <td><%= o.page_id  %></td>
  <td><%= o.name  %></td>
  <td><%= o.position  %></td>
  <td><%= o.visible  %></td>
  <td><%= o.content_type  %></td>
  <td><%= o.content  %></td>
  <td>
    <%= link_to 'show', section_path(o) %>
    <%= link_to 'edit', edit_section_path(o) %>
    <%= link_to 'delete', section_path(o), method: :delete %>
  </td>
</tr>
<% end %>

</table>
